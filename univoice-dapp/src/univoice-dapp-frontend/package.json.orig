{
  "name": "univoice-dapp-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create univoice-dapp-backend && dfx generate univoice-dapp-backend && dfx deploy",
    "start": "vite --port 5000",
    "dev": "vite serve --config vite.dev.config.js --no-clear",
    "prebuild": "dfx generate univoice-dapp-backend && dfx generate univoice-dapp-frontend",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "generate-certs": "chmod +x ./generate-certificates.sh && ./generate-certificates.sh",
    "start:https": "vite --port 5000",
    "serve": "node server/index.js"
  },
  "dependencies": {
    "@dfinity/agent": "0.15.6",
    "@dfinity/candid": "0.15.6",
    "@dfinity/identity": "0.15.6",
    "@dfinity/principal": "0.15.6",
    "@ldclabs/ic_oss_ts": "^1.0.1",
    "@lottiefiles/dotlottie-react": "^0.13.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-media-recorder-2": "1.6.23",
    "react-router-dom": "^6.10.0",
    "simple-zustand-devtools": "^1.1.0",
    "zustand": "^4.3.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^3.1.0",
    "dotenv": "^16.3.1",
    "sass": "^1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3"
  }
}
